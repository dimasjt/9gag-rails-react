{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Promise": true
    },
    "rules": {
        "comma-dangle": [
            "error",
            "always-multiline"
        ]
    }
}